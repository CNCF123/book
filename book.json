{
    "title": "Docker K8s",
    "description": "study docker & kubernetes book",
    "author": "Chen Kiaidi",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "我的主页": "http://www.chenkaidi.com"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus@^0.0.11",
        "-sharing",
        "advanced-emoji",
        "github",
        "github-buttons",
        "splitter@^0.0.8",
        "expandable-chapters-small@^0.1.7",
        "tbfed-pagefooter@^0.0.1",
        "simple-page-toc@^0.1.1",
        "donate@^1.0.2",
        "anchor-navigation-ex@0.1.8",
        "sectionx"
        
    ],

    "pluginsConfig": {
         "github": {
            "url": "https://github.com/xsdxn"
        },
        "github-buttons": {
            "repo": "xsdxn/dockerk8s",
            "types": [
                "star",
                "watch",
                "fork"
            ],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "<a href='http://www.miitbeian.gov.cn/publish/query/indexFirst.action'>苏ICP备18000953号-3</a>",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "alipay": "http://www.dockerk8s.cn/source/image/donate.jpg",
            "title": "",
            "button": "打赏",
            "alipayText": "支付宝"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "sectionx": {
          "tag": "b"
        }
    }
}